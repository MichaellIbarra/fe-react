# Variables de entorno para producción
# Copiar este archivo como .env.production.local y ajustar los valores

# Configuración de la aplicación
REACT_APP_NAME=EduAssist
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=production

# URLs de API (ajustar según tu configuración)
REACT_APP_API_BASE_URL=https://lab.vallegrande.edu.pe/school
REACT_APP_INSTITUTION_API_URL=https://lab.vallegrande.edu.pe/school/ms-institution/api/v1
REACT_APP_GRADE_API_URL=https://lab.vallegrande.edu.pe/school/ms-grade/api/v1
REACT_APP_STUDENT_API_URL=https://lab.vallegrande.edu.pe/school/ms-student/api/v1

# Configuración de build
GENERATE_SOURCEMAP=false
BUILD_PATH=build

# Configuración de optimización
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_SENTRY_DSN=

# Configuración de features
REACT_APP_ENABLE_DARK_MODE=true
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_PWA=false
